<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jian.shadow.mapper.SysPermissionMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.jian.shadow.entity.SysPermission">
        <id column="id" property="id" />
        <result column="resourceId" property="resourceId" />
        <result column="roleId" property="roleId" />
        <result column="permissionStatus" property="permissionStatus" />
        <result column="createDate" property="createDate" />
        <result column="orderBy" property="orderBy" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, resourceId, roleId, permissionStatus, createDate, orderBy
    </sql>

</mapper>
